@model COMP003B.Assignment4.Models.Quiz.TriviaQuiz


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Quiz:</h2>
<form asp-action="TriviaQuiz" method="post">
    <div>
        
        <label asp-for="Question1"> In what year did Ariana start her music career?</label>
        <input asp-for="Question1" class="form-control"/>
    </div>
    <div>
        <label asp-for="Question2">When is Ariana's birthday?</label>
        <input asp-for="Question2" class="form-control"/>
    </div>
    <div>
        <label asp-for="Question3">What state was Ariana born in?</label>
        <input asp-for="Question3" class="form-control"/>
    </div>
    <div>
        <label asp-for="Question4">How many albums does Ariana have?</label>
        <input asp-for="Question4" class="form-control"/>
    </div>
    <div>
        <label asp-for="Question5">How many siblings does Ariana have?</label>
        <input asp-for="Question5" class="form-control"/>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
